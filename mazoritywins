//function to find element with more appearances between two elements in an array
int majoritywins(int arr[], int x, int y)
{
  int cx = 0;
  int cy = 0;
  for(int i=0; i<n; i++)
  {
    if(arr[i]==x)
       cx++;
    else if(arr[i]==y)
       cy++;
   }
   if(cx>cy)
      return x;
   else
      return y;
   }
};
